<div class="page-container operator-page">
{{#link-to "feeds"}}
	Feed Registery 
{{/link-to}}
	>>{{model.name}}
	<h3 class = "agency-name">{{model.name}} {{#if model.short_name}}
	({{model.short_name}})
	{{/if}}
	</h3>
	<div class = "row">
			<table>
				<tr><td>Onestop id</td><td>{{model.onestop_id}}</td></tr>
				{{#if model.municipality}}
					<tr><td>Municipality</td><td>{{model.municipality}}</td></tr>
				{{/if}}
				<tr><td>State</td><td>{{model.state}}</td></tr>
				<tr><td>Country</td><td>{{model.country}}</td></tr>
				<tr><td>Website</td><td>{{model.website}}</td></tr>
				<tr><td>Timezone</td><td>{{model.timezone}}</td></tr>
				<tr><td>Identifiers</td><td>{{model.identifiers}}</td></tr>
				
			</table>
		
	</div>
	<div class = "row">
		<h3 class = "detail col-md-2">Feed Detail</h3>
		<div class = "col-md-10">
			{{#if model.feed }}
				<table>
					<tr><td>Onestop id</td><td>{{model.feed.onestop_id}}</td></tr>
					<tr><td>Data location</td><td>{{model.feed.url}}</td></tr>
					<tr><td>Format</td><td>{{model.feed.feed_format}}</td></tr>
					<tr><td>License name</td><td>{{model.feed.license_name}} ADD LINK OUT TO LICENSE 
					<!-- {{model.feed.license_url}} -->
					</td></tr>
					<tr><td>License attribution</td><td>{{model.feed.license_attribution}}</td></tr>
					<tr><td>License derivation</td><td>{{model.feed.license_derivation}}</td></tr>
					<tr><td>License redistribution</td><td>{{model.feed.license_redistribution}}</td></tr>
					<tr><td>Agency last checked on</td><td>{{format-date model.feed.last_fetched_at}}</td></tr>
					<tr><td>Feed updated on</td><td>{{format-date model.feed.last_imported_at}}</td></tr>
				</table>
			{{else}}
			  <h4>HEY YOU SHOULD WRITE GOOD SENTENCE TO CONVINCE PEOPLE TO UPLOAD </h4>
			<button> hello </button>
			{{/if}}
		</div>
	</div>
</div>