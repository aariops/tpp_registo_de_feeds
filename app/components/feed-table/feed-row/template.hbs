<td>
	{{#link-to "operator" operator}}
		{{operator.name}}
	{{/link-to}}
</td>
<td>{{operator.municipality}}</td>
<td>{{operator.feed.license_name}}</td>
<td>
<!-- the icon used depends on license details -->

	<div class = "license-attr">
		{{#if attributionUnknown}}
			<div class = "unknown-requires-attr"></div>			
		{{else if requiresAttribution}}
			<div class = "requires-attr"></div>
		{{else}}
			<div class = "not-requires-attr"></div>
		{{/if}}

		{{#if derivationUnknown}}
			<div class = "unknown-allow-derivation"></div>
		{{else if derivationAllowed}}
			<div class = "allow-derivation"></div>
		{{else}}
			<div class = "not-allow-derivation"></div>
		{{/if}}

		{{#if redistributionUnknown}}
			<div class = "unknown-allow-redistribution"></div>
		{{else if redistributionAllowed}}
			<div class = "allow-redistribution"></div>
		{{else}}
			<div class = "not-allow-redistribution"></div>
		{{/if}}
	</div>

</td>
