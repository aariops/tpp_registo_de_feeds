<td>
	{{#link-to "operator" operator}}
		{{operator.name}}
	{{/link-to}}
</td>
<td>{{operator.municipality}}</td>
<td>{{operator.feed.license_name}}</td>
<td>
<!-- the icon used depends on license details -->


		{{#if requiresAttribution}}        
			<img src="images/1_airport.png">
		{{else}}
			<img alt=": (">
		{{/if}}
		
		{{#if derivationAllowed}}
			<img src="images/2_taxi.png">
		{{else}}
			<img alt=": (">
		{{/if}}

		{{#if redistributionAllowed}}
			<img src="images/3_train.png">
		{{else}}
			<img alt=": (">
		{{/if}}


</td>
<td>
<!-- either single upload icon or two or three icons used here -->

	{{#unless operator.feed}}
		<img src="images/4_boat.png">
		{{else}}
		<img src="images/5_bus.png">
		<img src="images/6_park.png"><a {{bind-attr href='operator.feed.url'}}></a>
		<img src="images/7_gas_station.png">
	{{/unless}}

</td>