<td>
	{{#link-to "operator" operator}}
		{{operator.name}}
	{{/link-to}}
</td>
<td>{{operator.municipality}}</td>
<td>{{operator.feed.license_name}}</td>
<td>
<!-- the icon used depends on license details -->
	<table>
	<!-- <td><img {{bind-attr src="images/clipboard-dark.svg"}} alt="Requires Attribution"></td>  --> 
		{{#if requiresAttribution}}        
			<td><img src="images/1_airport.png"></td>
		{{else}}
			<td><img alt=": ("></td>
		{{/if}}
		<!-- <img class="avatar" src="/images/avatar.jpg" /> -->
		{{#if derivationAllowed}}
			<td><img src="images/2_taxi.png"></td>
		{{else}}
			<td><img alt=": ("></td>
		{{/if}}
		{{#if redistributionAllowed}}
			<td><img src="images/3_train.png"></td>
		{{else}}
			<td><img alt=": ("></td>			
		{{/if}}
	</table>
</td>
<td>
<!-- either single upload icon or two or three icons used here -->
	<table>
	{{#unless operator.feed}}
		<td><img src="images/4_boat.png"></td>
		{{else}}
		<td><img src="images/5_bus.png"></td>
		<td><img src="images/6_park.png"><a {{bind-attr href='operator.feed.url'}}></a></td>
		<td><img src="images/7_gas_station.png"></td>
	{{/unless}}
	</table>
</td>