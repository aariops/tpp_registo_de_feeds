<table class="table table-striped">
  <thead>
    <tr>
      <th {{action "sortBy" "name"}}>Agency Name</th>
      <th {{action "sortBy" "municipality"}}>Region</th>
      <th {{action "sortBy" "feed.license_name"}}>License</th>
      <th>License Details</th>
      <th>Links</th>
    </tr>
  </thead>
  <tbody>
    {{#each operator in sortedOperators}}
  	  {{feed-table/feed-row operator=operator}}
  	{{/each}} 
  </tbody>
</table>
